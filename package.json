{
  "name": "template-tanstack-router",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "imports": {
    "#app/*": "./app/*",
    "#tests/*": "./tests/*"
  },
  "scripts": {
    "build": "vite build && tsc",
    "coverage": "vitest run --coverage",
    "dev": "cross-env NODE_ENV=development MOCKS=true vite",
    "dev:no-mocks": "cross-env NODE_ENV=development vite",
    "format": "prettier --write .",
    "lint": "eslint . --report-unused-disable-directives --max-warnings=0",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "husky",
    "serve": "vite preview",
    "start": "vite",
    "test": "vitest",
    "test:e2e": "npm run --silent test:e2e:dev",
    "test:e2e:dev": "playwright test --ui",
    "test:e2e:install": "npx playwright install --with-deps chromium",
    "test:e2e:run": "cross-env CI=true playwright test",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "typecheck:watch": "npm run typecheck -- --watch",
    "validate": "npm run lint && npm run type-check && npm run coverage"
  },
  "dependencies": {
    "@tailwindcss/vite": "4.1.11",
    "@tanstack/react-query": "5.81.5",
    "@tanstack/react-query-devtools": "5.81.5",
    "@tanstack/react-router": "1.123.2",
    "@tanstack/router-plugin": "1.123.2",
    "axios": "1.10.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwindcss": "4.1.11",
    "zod": "3.25.67"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@commitlint/types": "19.8.1",
    "@epic-web/config": "1.21.0",
    "@playwright/test": "1.53.2",
    "@tanstack/eslint-plugin-query": "5.81.2",
    "@tanstack/react-router-devtools": "1.123.2",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@vitejs/plugin-react": "4.6.0",
    "@vitest/coverage-v8": "3.2.4",
    "cross-env": "7.0.3",
    "eslint": "9.30.0",
    "eslint-config-prettier": "10.1.5",
    "husky": "9.1.7",
    "jsdom": "26.1.0",
    "lint-staged": "16.1.2",
    "prettier": "3.6.2",
    "typescript": "5.8.3",
    "vite": "6.3.5",
    "vitest": "3.2.4",
    "web-vitals": "5.0.3"
  }
}
